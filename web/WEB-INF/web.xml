<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>DOMParserServlet</servlet-name>
        <servlet-class>by.epamtc.iovchuk.servlet.DOMParserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SAXParserServlet</servlet-name>
        <servlet-class>by.epamtc.iovchuk.servlet.SAXParserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StAXParserServlet</servlet-name>
        <servlet-class>by.epamtc.iovchuk.servlet.StAXParserServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DOMParserServlet</servlet-name>
        <url-pattern>/dom-parse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SAXParserServlet</servlet-name>
        <url-pattern>/sax-parse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StAXParserServlet</servlet-name>
        <url-pattern>/stax-parse</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>parsers.jsp</welcome-file>
    </welcome-file-list>

</web-app>